{
  "name": "themanaworld-website",
  "version": "5.0.0",
  "engines": {
    "node": ">=11.7.0"
  },
  "private": true,
  "scripts": {
    "serve": "npx vue-cli-service serve",
    "build": "npx vue-cli-service build --modern",
    "build:dev": "npx vue-cli-service build --mode development",
    "test:unit": "npx vue-cli-service test:unit",
